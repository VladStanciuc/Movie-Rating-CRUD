@using System.Data
@{
    ViewBag.Title = "DouaCaseProductie";
    DataTable table = ViewBag.CaseProductie;
}

<h2>Filme cu cel putin 3 case de productie</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <td>Titlu</td>
        </tr>
    </thead>
    <tbody>
        @foreach (DataRow currentRow in table.Rows)
        {
            <tr>
                <td>@currentRow["Nume"] </td>
            </tr>
        }
    </tbody>
</table>

